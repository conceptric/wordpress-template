{
	"name": "termly",
	"description": "Termly WordPress Plugin",
	"version": "1.0.0",
	"scripts": {
		"start": "parcel watch ./src/css/termly.scss ./src/css/custom-controls/custom-controls.scss ./src/js/cookie-management.js ./src/js/banner-settings.js --public-url ../ --no-cache",
		"build": "parcel build ./src/css/termly.scss ./src/css/custom-controls/custom-controls.scss ./src/js/cookie-management.js ./src/js/banner-settings.js --public-url ../ --no-cache",
		"clean": "rm -rf dist/*"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/resumecompanion/wordpress-plugin"
	},
	"author": "Pixel Jar",
	"license": "ISC",
	"devDependencies": {
		"node-sass": "^5.0.0",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.32.7"
	}
}
